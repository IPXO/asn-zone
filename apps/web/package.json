{
  "name": "asn-zone-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app --ext .ts,.tsx",
    "mkroute": "node ./scripts/mkroute.mjs",
    "format": "prettier --write \"app/**/*.{ts,tsx,css}\" \"components/**/*.tsx\" \"lib/**/*.{ts,tsx}\" \"scripts/**/*.{js,mjs,sh}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,css}\" \"components/**/*.tsx\" \"lib/**/*.{ts,tsx}\" \"scripts/**/*.{js,mjs,sh}\"",
    "export:local": "rm -rf out && next build",
    "dataset:build": "ts-node --transpile-only ../../tools/build-dataset.ts"
  },
  "dependencies": {
    "next": "^14.2.32",
    "next-themes": "^0.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-schemaorg": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "20.12.12",
    "@types/react": "18.2.66",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.32",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "5.5.4"
  }
}
